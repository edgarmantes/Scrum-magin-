{
  "name": "scrumapp",
  "version": "0.0.1",
  "description": "Scrum app",
  "main": "js/index.js",
  "scripts": {
    "start": "http-server -c-1 public",
    "dev": "run-p watch serve",
    "test": "mocha --recursive --compilers js:babel-core/register --require ./test/test_helper.js --recursive ./test",
    "test:watch": "npm run test -- --watch",
    "build": "run-s build:*",
    "clean": "rimraf public",
    "serve": "npm start",
    "watch": "run-p watch:*",
    "mkdir": "mkdirp public",
    "clean:html": "rimraf public/index.html",
    "mkdir:html": "run-s mkdir",
    "build:html": "run-s clean:html mkdir:html _build:html",
    "_build:html": "cpr index.html public/index.html",
    "watch:html": "run-s build:html _watch:html",
    "_watch:html": "chokidar index.html -c \"npm run build:html\"",
    "clean:js": "rimraf public/js",
    "mkdir:js": "mkdirp public/js",
    "build:js": "run-s clean:js mkdir:js _build:js",
    "_build:js": "webpack",
    "watch:js": "run-s build:js _watch:js",
    "_watch:js": "webpack --watch",
    "mkdir:images": "run-s mkdir",
    "build:images": "run-s clean:images mkdir:images _build:images",
    "_build:images": "cp -R images public/images",
    "clean:images": "rimraf public/images",
    "clean:css": "rimraf public/css",
    "mkdir:css": "run-s mkdir",
    "build:css": "run-s clean:css mkdir:css _build:css",
    "_build:css": "cpr css public/css",
    "watch:css": "run-s build:css _watch:css",
    "_watch:css": "chokidar css/**/* -c \"npm run build:css\"",
    "mkdir:assets": "run-s mkdir",
    "clean:assets": "rimraf public/assets",
    "build:assets": "run-s clean:assets mkdir:assets _build:assets",
    "_build:assets": "cpr assets public/assets",
    "watch:assets": "run-s build:assets _watch:assets",
    "_watch:assets": "chokidar assets/**/* -c \"npm run build:assets\""
  },
  "author": "Joe Turner <joe@oampo.co.uk>",
  "license": "UNLICENSED",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.13.1",
    "babel-preset-react": "^6.11.1",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.5.1",
    "chai-http": "^3.0.0",
    "chai-jquery": "^2.0.0",
    "chokidar-cli": "^1.2.0",
    "cpr": "^2.0.0",
    "enzyme": "^2.4.1",
    "http-server": "^0.9.0",
    "jquery": "^3.2.1",
    "jsdom": "^8.1.0",
    "mkdirp": "^0.5.1",
    "mocha": "^3.2.0",
    "npm-run-all": "^3.0.0",
    "redux-devtools-chart-monitor": "^1.6.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "bcryptjs": "^2.4.0",
    "bluebird": "^3.5.0",
    "body-parser": "^1.13.3",
    "connect-mongo": "^1.3.2",
    "cookie-parser": "^1.3.5",
    "es6-promise": "^4.1.0",
    "express": "^4.13.3",
    "express-session": "^1.11.3",
    "fetch": "^1.1.0",
    "isomorphic-fetch": "^2.2.1",
    "mongoose": "^4.7.3",
    "passport": "^0.3.0",
    "passport-http": "^0.3.0",
    "passport-local": "^1.0.0",
    "qwest": "^4.4.5",
    "react": "^15.3.1",
    "react-dom": "^15.5.4",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0"
  }
}
